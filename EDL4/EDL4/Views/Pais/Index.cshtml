@model IEnumerable<EDL4.Models.Pais>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @using (Html.BeginForm("carga", "Pais", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        <span>Seleccionar archivo:</span>
        <input type="file" name="postedFile" />
                    <br />
                    <input type="submit" value="Cargar Archivo de paises" />

    }
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nombre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nombre)
        </td>
        <td>
          
            @Html.ActionLink("Estampas para cambiar,coleccionadas y faltantes", "Details", new { id=item.nombre }) |
        
        </td>
    </tr>
}

</table>
