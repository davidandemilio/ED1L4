@model IEnumerable<EDL4.Models.Estampa>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @using (Html.BeginForm("carga", "Estampa", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        <span>Seleccionar archivo:</span>
        <input type="file" name="postedFile" />
        <br />
                    <input type="submit" value="Cargar Archivo de estampas" />

    }
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.pais_no)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.se_tiene)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.pais_no)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.se_tiene)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.pais_no }) |
           
        </td>
    </tr>
}

</table>
